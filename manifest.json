{
    "name": "Getting CPU and RAM usage - v3",
    "description": "Getting CPU and RAM usage - v3",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "permissions": [
        "webNavigation",
        "activeTab",
        "scripting",
        "system.cpu",
        "system.memory",
        "tabs"
    ],
    "host_permissions": [
        "http://localhost:4200/"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
}